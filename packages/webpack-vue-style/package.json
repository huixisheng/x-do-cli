{
  "name": "@x-scaffold/webpack-vue-style",
  "version": "0.0.1",
  "description": "根据[vuejs-templates/webpack]提取相关style的配置",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run eslint && ./node_modules/mocha/bin/mocha --require babel-register --timeout 30000",
    "test:watch": "npm run eslint && ./node_modules/mocha/bin/mocha --require babel-register --watch  --timeout 30000",
    "build": "babel index.js --presets babel-preset-es2015 --out-dir dist",
    "eslint:diff": "node_modules/eslint/bin/eslint.js $(git diff-index HEAD --name-only --diff-filter ACMR |grep '\\.\\(jsx\\|es6\\|js\\|vue\\)$')",
    "eslint": "cross-env NODE_ENV=production node_modules/eslint/bin/eslint.js --color --max-warnings 10 --ext .js,.vue static src build config test fis-conf.js",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "keywords": [],
  "author": "huixisheng <huixisheng@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "extract-text-webpack-plugin": "^3.0.2",
    "css-loader": "^0.28.11",
    "glob": "^7.1.2",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^7.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-style-loader": "^4.1.0",
    "webpack": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.5",
    "eslint": "^4.18.2",
    "eslint-config-style-guide": "0.0.6",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.11",
    "husky": "^0.15.0-rc.13",
    "mocha": "^5.0.4",
    "node-plus-string": "^1.0.1",
    "x-config-deploy": "^0.0.3"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
